{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "header-row#contact-desktop",
      "header-row#logo-desktop",
      "header-row#menu-desktop"
    ]
  },
  //CONTACT
  "header-row#contact-desktop": {
    "children": [
      "header-spacer",
      "vtex.menu@2.x:menu#contactmenu"
      // "rich-text#attendance",
      // "rich-text#talk",
      // "rich-text#login",
      // "rich-text#pedidos",
      // "rich-text#cadastro"
    ]
  },
  "vtex.menu@2.x:menu#contactmenu": {
    "children": [
      "menu-item#phone",
      "menu-item#requests",
      "menu-item#register"
    ],
    "props": {
      "blockClass": "menu-contact",
      "inverted": "true"
    }
  },
  "menu-item#phone": {
    "props": {
      "text": "ATENDIMENTO (41) 3272-8789"
    }
  },
  "menu-item#requests": {
    "props": {
      "text": "MEUS PEDIDOS",
      "href": "/account/#orders"
    }
  },
  "menu-item#register": {
    "props": {
      "text": "MEU CADASTRO",
      "href": "/account"
    }
  },
  //LOGO
  "header-row#logo-desktop": {
    "children": [
      "logo#desktop",
      "rich-text#facebook",
      "rich-text#twiter",
      "rich-text#instagram",
      "header-spacer",
      "search-bar",
      "header-spacer",
      "minicart"
    ],
    "props": {
      "sticky": true,
      "blockClass": "main-header"
    }
  },
  "rich-text#facebook":{
"props":{
  "text":"[![Bebellys](https://bebellys.vteximg.com.br/arquivos/facetop.png 'Facebook Bebellys')](https://www.facebook.com/bebellys.infantil)"

}
},
"rich-text#twiter":{
"props":{
  "text":"[![Bebellys](https://bebellys.vteximg.com.br/arquivos/twitertop.png 'Twitter Bebellys')](https://twitter.com/bebellysmoda/with_replies)"
}
},
 
"rich-text#instagram":{
    "props":{
      "text":"[![Bebellys](https://bebellys.vteximg.com.br/arquivos/instagramtop.png 'Instagram Bebellys')](https://www.instagram.com/bebellysmoveis/)"
    }
},

  "search-bar": {
    "props": {
      "iconClasses": "searchicon"
    }
  },
  //MENU
  "header-row#menu-desktop": {
    "children": [
      "vtex.menu@2.x:menu#prior"
    ],
    "props": {
      "blockClass": "menu-link",
      "inverted": "true"
    }
  },
  "vtex.menu@2.x:menu#prior": {
    "children": [
      "menu-item#bercos",
      "menu-item#comodas",
      "menu-item#roupeiros",
      "menu-item#marcas",
      "menu-item#quartos",
      "menu-item#offer"
    ]
  },
  "menu-item#bercos": {
    "blocks": [
      "vtex.menu@2.x:submenu#bercossub"
    ],
    "props": {
      "text": "BERÇOS",
      "href": "/bercos"
    }
  },
  "vtex.menu@2.x:submenu#bercossub": {
    "children": [
      "vtex.menu@2.x:menu#submenubercos"
    ]
  },
  "vtex.menu@2.x:menu#submenubercos": {
    "children": [
      "menu-item#colchoes"
    ]
  },
  "menu-item#colchoes": {
    "props": {
      "id": "menu-item-colchoes",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/bercos/colchao",
        "noFollow": true,
        "tagTitle": "Colchões",
        "text": "Colchões"
      }
    }
  },
  "menu-item#comodas": {
    "blocks": [
      "vtex.menu@2.x:submenu#subcomodas"
    ],

    
    "props": {
      "text": "COMODAS",
      "href":"/comodas"
      // "blockClass": "highligthstop"
    }
  },
  "vtex.menu@2.x:submenu#subcomodas": {
    "children": [
      "vtex.menu@2.x:menu#comodassub"
    ]
  },
  "vtex.menu@2.x:menu#comodassub": {
    "children": [
      "menu-item#comodasbb"
    ]
  },
  "menu-item#comodasbb": {
    "props": {
      "id": "menu-item-comodas",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/comodas/comodas-para-casal",
        "noFollow": true,
        "tagTitle": "Comodas para bebê",
        "text": "Comodas para bebê"
      }
    }
  },
  "menu-item#offer": {
    "props": {
      "text": "PROMOÇÃO DA SEMANA",
      "blockClass": "offerstop",
      "href":"/promocao-da-semana"
    }
  },
  //ROUPEIROS
