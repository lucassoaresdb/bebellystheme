  "logo#desktop": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "https://bebellys.vteximg.com.br/arquivos/Bebellys_logo.png",
      "width": "180"
    }
  },
  "minicart": {
    "blocks": [
      "product-summary"
    ],
    "props": {
      "type": "popup",
      "showRemoveButton": true,
      "showDiscount": true,
      "showSku": true,
      "labelMiniCartEmpty": "",
      "labelButtonFinishShopping": "Go to checkout",
      "enableQuantitySelector": true,
      "maxQuantity": 10,
      "labelClasses": "gray",
      "iconClasses": "MinicartIcon"
    }
  },
  "header-layout.mobile": {
    "children": [
      "header-row#1-mobile"
    ]
