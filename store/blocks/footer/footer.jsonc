    "props": {
      "blockClass": "payment-methods"
    }
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": [
        "MasterCard", "Visa", "Diners Club"
      ]
    }
  },
  "rich-text#paymenttitle":{
    "props":{
      "text": "PAGAMENTO",
      "blockClass": "footertitle"
    }
  },

  "flex-layout.col#social":{
    "children":[
      "rich-text#socialtitle",
      "social-networks"
    ]
  },
  "rich-text#socialtitle":{
    "props":{
      "text": "ACOMPANHE",
      "blockClass": "footertitle"
    }
  },

  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/bebellys.infantil/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/bebellysmoveis/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/login?redirect_after_login=%2Fbebellysmoda%2Fwith_replies"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com"
        }
      ]
    }
  },

  "footer-row#2-desktop": {
    "children":[
      "flex-layout.row#middlefoot"
    ]
  },
  "flex-layout.row#middlefoot":{
    "children":[
      "flex-layout.col#help",
