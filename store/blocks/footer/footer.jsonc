{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "rich-text#whatsapp",
      "footer-row#1-desktop",
      "footer-row#2-desktop",
      "footer-row#3-desktop"
    ]
  },
  "rich-text#whatsapp":{
    "props":{
      "text": "[whatsApp](https://api.whatsapp.com?target=_blank/send?phone=4132728789&text=&source=&data=)",
      "blockClass":"whatsapp"
    }
  },
  "footer-row#1-desktop": {
    "children": [
      "flex-layout.row#topfoot"
    ]
  },
  "flex-layout.row#topfoot":{
    "children":[
      "flex-layout.col#analoren",
      "footer-spacer",
      "flex-layout.col#payment",
      "footer-spacer",
      "flex-layout.col#social"
    ],
    "props": {
      "blockClass": "topfootrow"
    }
  },
  "flex-layout.col#analoren":{
    "children":[
      "rich-text#analorentitle",
      "rich-text#about",
      "rich-text#privacy"
      // "rich-text#talk"
    ]
  },
  "rich-text#analorentitle":{
    "props":{
      "text": "A BEBELLYS",
      "blockClass": "footertitle"
    }
  },
  "rich-text#about":{
    "props":{
      "text": "[Sobre a Bebellys](/about-us)",
      "blockClass": "footlinkitem"
    }
  },
  "rich-text#privacy":{
    "props":{
      "text": "[Privacidade](/privacy-info)",
      "blockClass": "footlinkitem"

    }
  },

  "flex-layout.col#payment":{
    "children":[
      "rich-text#paymenttitle",
      "accepted-payment-methods"
